<form method="post" action="{{ path('account_delete', {'id': account.id}) }}" onsubmit="return confirm('Czy jesteś pewny że chcesz usunąć to konto?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ account.id) }}">
    <button class="btn">Usuń</button>
</form>
